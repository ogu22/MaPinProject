<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">


    <ImageView
        android:id="@+id/pimageView5"
        android:layout_width="190dp"
        android:layout_height="131dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="107dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/progress_horizontal" />

    <ImageView
        android:id="@+id/pimageView12"
        android:layout_width="190dp"
        android:layout_height="131dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/progress_horizontal" />

    <ImageView
        android:id="@+id/pimageView2"
        android:layout_width="65dp"
        android:layout_height="87dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_revert" />

    <Button
        android:id="@+id/pbutton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="28dp"
        android:text="編集画面へ"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ptextView12"
        android:layout_width="246dp"
        android:layout_height="28dp"
        android:layout_marginStart="92dp"
        android:layout_marginBottom="504dp"
        android:text="名前"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/pbutton8"
        android:layout_width="213dp"
        android:layout_height="48dp"
        android:layout_marginBottom="421dp"
        android:text="自分の投稿"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/pbutton9"

        android:layout_width="196dp"
        android:layout_height="49dp"
        android:layout_marginBottom="420dp"
        android:text="お気に入り"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/pimageView6"
        android:layout_width="158dp"
        android:layout_height="145dp"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="6dp"
        app:layout_constraintBottom_toTopOf="@+id/ptextView12"
        app:layout_constraintEnd_toStartOf="@+id/pbutton7"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/pimageView2"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <Button
        android:id="@+id/pbutton"
        android:layout_width="262dp"
        android:layout_height="38dp"
        android:layout_marginStart="86dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="35dp"
        android:text="▼さらに表示する"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="174dp"
        android:layout_height="147dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pbutton9"
        app:srcCompat="@drawable/ic_launcher_background" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="145dp"
        android:layout_height="163dp"
        android:layout_marginStart="39dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/pbutton"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/checkbox_on_background" />

</androidx.constraintlayout.widget.ConstraintLayout>